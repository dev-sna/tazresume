                      </div>
                  </div>
                  
                  
                  <div class="row data-container">
                      <div class="col-xs-4 data-date">
                          <p><%= eperiodThree %></p>
                      </div>
                      <div class="col-xs-8 data-data">
                          <p class="iName"><%= orgyNameThree %></p>
                          <p><%= jobTitleThree %></p>
                      </div>
                  </div>
                  
                  
                  <div class="row data-container">
                      <div class="col-xs-4 data-date">
                          <p><%= eperiodFour %></p>
                      </div>
                      <div class="col-xs-8 data-data">
                          <p class="iName"><%= orgyNameFour %></p>
                          <p><%= jobTitleFour %></p>
                      </div>
                  </div>
              </div>
          </section>
          <!-- EDU AND WORK ENDS HERE -->
          <!-- SKILLS STARTS HERE -->
          <section class="row" id="skills">
              <p id="ew-heading">Skills</p>
              
             <div class="col-xs-6" id="skill-container-border">
                      
                      <div class="row skills-container">
                          <div class="col-xs-4 ">
                            <p><%= skOne %></p>
                          </div>
                          <div class="col-xs-8">
                            <p>
                                <% for(var sk1 = 0; sk1 < lvOne; sk1++) { %>
