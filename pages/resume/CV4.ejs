                  </div>
              </div>
              
              <!-- WORK -->
              <div class="col-xs-6" id="work">
                  
                  <p id="ew-heading">Where I've Worked</p>
                  
                  <div class="row data-container">
                      <div class="col-xs-4 data-date">
                          <p><%= eperiodOne %></p>
                      </div>
                      <div class="col-xs-8 data-data">
                          <p class="iName"><%= orgyNameOne %></p>
                          <p><%= jobTitleOne %></p>
                      </div>
                  </div>
                  
                  <div class="row data-container">
                      <div class="col-xs-4 data-date">
                          <p><%= eperiodTwo %></p>
                      </div>
                      <div class="col-xs-8 data-data">
                          <p class="iName"><%= orgyNameTwo %></p>
