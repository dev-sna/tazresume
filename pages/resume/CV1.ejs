    <div class="col-xs-1"></div>
        <div class="col-xs-5">
        <div class="row">
        <div class="col-xs-4"><p class="skill-text"><%=  skOne%></p></div>
        
        <div class="col-xs-8" id="stars">
        <p>
            <% for(var sk1 = 0; sk1 < lvOne; sk1++) { %>
                                <i class="fa fa-star" aria-hidden="true"></i>
                                
                                <% } %></p>
        </div>
